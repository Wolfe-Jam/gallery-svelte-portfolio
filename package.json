{
  "name": "gallery-svelte-portfolio",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "package:flipcard": "svelte-package -o dist/flipcard",
    "build:analyze": "vite build && npx vite-bundle-analyzer dist",
    "lint:svelte5": "svelte-check --threshold error --diagnostic-sources js,svelte",
    "perf:bundle": "npm run build && du -sh dist/*"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/svelte": "^4.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.10.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^3.3.0",
    "@supabase/supabase-js": "^2.52.0",
    "@sveltejs/adapter-vercel": "^5.8.0",
    "@tailwindcss/typography": "^0.5.16",
    "cloudinary": "^2.7.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "tailwindcss": "^4.1.11"
  },
  "description": "Professional UI Component Library Portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolfe-Jam/gallery-svelte-portfolio.git"
  }
}